{
  "clearMocks": true,
  "collectCoverage": true,
  "coverageDirectory": "coverage",
  "coverageReporters": ["json", "text", "lcov", "clover", "json-summary"],
  "moduleFileExtensions": ["js", "jsx", "ts", "tsx", "json", "node"],
  "moduleNameMapper": {
    "\\.(css|less)$": "identity-obj-proxy",
    "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "@nanshu/react-test-preset/__mocks__/file-mock.js"
  },
  "setupFilesAfterEnv": ["./node_modules/@nanshu/react-test-preset/jest.setup.ts"],
  "testEnvironment": "jsdom",
  "transformIgnorePatterns": ["/node_modules/?!(react-markdown)"]
}
